{
  "name": "scss_basics",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel sass browsersync",
    "sass": "node-sass --output-style expanded --source-map true --source-map-contents true --precision 6 scss/main.scss css/main.css  --watch scss/*  ",
    "browsersync": "browser-sync start --server --files \"*.html, css/*.css\""
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "browser-sync": "^2.26.14",
    "hosted-git-info": "^4.0.2",
    "node-sass": "^5.0.0",
    "npm-run-all": "^4.1.5"
  }
}
